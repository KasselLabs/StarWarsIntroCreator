{
  "name": "star-wars-intro-creator",
  "version": "2.0.0",
  "description": "Star Wars Intro Creator by Kassel Labs",
  "author": "Kassel Labs <contact@kassellabs.io>",
  "license": "MIT",
  "scripts": {
    "clear": "rm -rf dist .cache public",
    "build": "yarn clear && parcel build ./src/index.html",
    "build-gitlab": "yarn build -d public --public-url /starwarsintrocreator-stage",
    "start": "parcel ./src/index.html -p 8080 --no-hmr",
    "test": "jest",
    "test:watch": "yarn test --watchAll",
    "test:coverage": "yarn test --coverage",
    "test:coverage:results": "google-chrome coverage/lcov-report/index.html",
    "lint": "eslint src/",
    "deploy": "yarn build && cp CNAME dist/ && gh-pages -d dist -m "
  },
  "jest": {
    "testURL": "http://localhost/",
    "moduleDirectories": [
      "node_modules",
      "src"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.2",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "gh-pages": "^1.1.0",
    "jest": "^22.4.4",
    "parcel-bundler": "^1.12.3",
    "stylint": "^1.5.9",
    "stylus": "^0.54.5"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-preset-react": "^6.24.1",
    "bowser": "^1.9.4",
    "lodash.isequal": "^4.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "lodash.uniq": "^4.5.0",
    "sweetalert2": "^7.15.1"
  }
}
