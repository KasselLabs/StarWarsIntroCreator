{
  "presets": [
    ["env", {
      "target": {
        "browsers": ["last 2 versions", "ie 11"]
      }
    }],
    "react"
  ],
  "plugins": [
    "transform-object-rest-spread",
    ["transform-runtime", {
      "helpers": false,
      "polyfill": false,
      "regenerator": true,
      "moduleName": "babel-runtime"
    }],
    "transform-class-properties",
    "transform-react-jsx"
  ]
}